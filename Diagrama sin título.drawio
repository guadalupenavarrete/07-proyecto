        <mxCell id="i4EwPONDSbQexWhAOyuk-18" value="&lt;h5&gt;ayuda&lt;/h5&gt;" style="text;html=1;resizable=0;autosize=1;align=center;verticalAlign=middle;points=[];fillColor=none;strokeColor=none;rounded=0;labelBackgroundColor=none;labelBorderColor=none;strokeWidth=0;fontFamily=Comic Sans MS;fontSize=21;fontColor=default;" parent="i4EwPONDSbQexWhAOyuk-12" vertex="1">
